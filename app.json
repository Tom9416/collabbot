{
  "name": "CTM-CollabBot",
  "description": "A Slack bot to collaborate",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "COLLABHELP_COMMAND_TOKEN": {
      "description": "Slash command token, for the collabhelp command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
